{
  "name": "@loanlink-nl/libxmljs2",
  "author": "LoanLink",
  "contributors": [
    "Jeff Smick",
    "Marco Rogers"
  ],
  "binary": {
    "module_name": "xmljs",
    "module_path": "./build/Release/",
    "host": "https://github.com",
    "remote_path": "./loanlink-nl/libxmljs2/releases/download/v{version}/",
    "package_name": "{node_abi}-{platform}-{arch}-{libc}.tar.gz"
  },
  "description": "libxml bindings for v8 javascript engine",
  "version": "0.37.0",
  "scripts": {
	  "prepare": "bun run build",
	  "build": "node-pre-gyp rebuild",
	  "install": "node-pre-gyp install --fallback-to-build --loglevel http",
	  "tsd": "tsd"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/loanlink-nl/libxmljs2.git"
  },
  "bugs": {
    "url": "http://github.com/loanlink-nl/libxmljs2/issues"
  },
  "main": "./index",
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "src",
    "lib",
    "vendor",
    "binding.gyp"
  ],
  "dependencies": {
		"bindings": "1.5.0"
  },
  "devDependencies": {
		"@mapbox/node-pre-gyp": "2.0.3",
		"node-addon-api": "8.5.0",
		"node-gyp": "12.1.0",
		"tsd": "0.33.0",
		"typescript": "5.9.3"
  }
}


